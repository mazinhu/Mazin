package sessopm2.oopLibrary;

public class Library {

Books[] allbooks = new Books[5];




public static void main (String arguments[]){
	
	Library l = new Library();
	//l.allbooks[0]
	Books book1= new Books("Book1", "author1", 10);	
	Books book2= new Books("Book2", "author2", 20);
	l.allbooks[0]= book2;
	l.allbooks[1]=book1;
	Books book3= new Books("Book3", "author3", 30);
	l.allbooks[2]= book3;
	Books book4= new Books("Book4", "author4", 40);
	l.allbooks[0]= book4;
	Books book5= new Books("Book5", "author5", 50);
	l.allbooks[0]= book5;
}

}
